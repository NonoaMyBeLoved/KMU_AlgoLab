주어진 정수의 합 구하기

#include <iostream>
using namespace std;
int main()
{
    int t; 
    cin >> t; 

    while (t--)
    { 
        int n; 
        cin >> n; 
        int sum = 0; 

        for (int i = 0; i < n; i++)
        { 
            int num;
            cin >> num;
            sum += num;
        }
        cout << sum << endl;
    }

	return 0;
}
