#include <iostream>
using namespace std;

int main()
{
    int t;
    cin >> t;
    for (int i = 0; i < t; i++)
    {
        int doro;        
        cin >> doro;
        int zero = 0, one = 0;
        while (doro > 0)
        {
            
            if (doro % 2 == 0) zero++;
            else one++;
            doro /= 2;
            
        }
        cout << zero << " " << one << "\n";
    }
    return 0;
}
